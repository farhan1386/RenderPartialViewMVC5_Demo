@model IEnumerable<RenderPartialView_Demo.Models.Customer>

@{
    ViewBag.Title = "Index";
}

<h4 class="text-uppercase text-center">List of customer</h4>

<ul class="nav nav-pills">
    <li class="nav-item">
        <a class="nav-link active" data-toggle="pill" href="#home">Partial</a>
    </li>
    <li class="nav-item">
        <a class="nav-link" data-toggle="pill" href="#menu1">Render Partial</a>
    </li>
    <li class="nav-item">
        <a class="nav-link" data-toggle="pill" href="#menu2">Action</a>
    </li>
    <li class="nav-item">
        <a class="nav-link" data-toggle="pill" href="#menu3">Render Action</a>
    </li>
    <li class="nav-item">
        <a class="nav-link" data-toggle="pill" href="#menu4">Jquery</a>
    </li>
</ul>

<div class="tab-content m-2">
    <div class="tab-pane container active" id="home">
        @Html.Partial("_Customers")
    </div>
    <div class="tab-pane container fade" id="menu1">
        @{ 
            Html.RenderPartial("_Customers");
        }
    </div>
    <div class="tab-pane container fade" id="menu2">
        @Html.Action("Customers")
    </div>
    <div class="tab-pane container fade" id="menu3">
        @{ 
            Html.RenderAction("Customers");
        }
    </div>
    <div class="tab-pane container fade" id="menu4">
        <div id="loadCustomer"></div>
    </div>
</div>
<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $('#loadCustomer').load('@Url.Content("/Home/Customers")');
    });
</script>



